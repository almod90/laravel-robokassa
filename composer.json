{
    "name": "almod90/robokassa",
    "description": "Laravel Robokassa",
    "keywords": [
        "laravel",
        "robokassa"
    ],
    "homepage": "https://github.com/almod90/robokassa",
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Modin",
            "email": "almod90@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Viacheslav Ostrovskiy",
            "email": "chelout@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0",
        "laravel/framework": "~5.5",
        "lexty/robokassa": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0 || ~8.0",
        "laravel/laravel": "~5.5",
        "mockery/mockery": "^0.9.1"
    },
    "autoload": {
        "psr-4": {
            "Almod90\\Robokassa\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Almod90\\Robokassa\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Almod90\\Robokassa\\RobokassaServiceProvider"
            ],
            "aliases": {
                "Robokassa": "Almod90\\Robokassa\\Facade"
            }
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
